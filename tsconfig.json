{
  "extends": "./paths.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "strict": true,
    "jsx": "react-jsx",
    "typeRoots": ["./types"]
  }
}
